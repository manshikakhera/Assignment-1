Q10
#include <iostream>
#include <string>

int main() {
    std::string inputDate;
    std::cout << "Enter date in DD/MM/YYYY format: ";
    std::getline(std::cin, inputDate);

    std::string day = inputDate.substr(0, 2);
    std::string month = inputDate.substr(3, 2);
    std::string year = inputDate.substr(6, 4);

    std::cout << "Day - " << day << " , Month - " << month << " , Year - " << year << std::endl;

    return 0;
}
