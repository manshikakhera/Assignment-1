Q4
#include <iostream>
#include <cmath>

int main() {
    double radius;
    std::cout << "Enter the radius of the circle: ";
    std::cin >> radius;

    double area = M_PI * radius * radius;
    std::cout << "Area of circle is " << area << " having the radius " << radius << std::endl;

    return 0;
}
Q5
#include <iostream>
#include <cstring>

int main() {
    char str[100];
    std::cout << "Enter a string: ";
    std::cin.getline(str, 100);

    int length = strlen(str);
    std::cout << "Length of the string is " << length << std::endl;

    return 0;
}
Q6
#include <iostream>
#include <string>

int main() {
    std::string name;
    std::cout << "Enter your name: ";
    std::getline(std::cin, name);

    std::cout << "Hello, \"" << name << "\"" << std::endl;

    return 0;
}
